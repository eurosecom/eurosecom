				</xsd:complexType>
			</xsd:element>
			<xsd:element name="suhlasZaslat" type="only_0_1"/>
			<xsd:element name="osobitneZaznamy">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="uvadza" type="only_0_1"/>
						<xsd:element name="udajeOprijmoch" type="udajeOprijmoch" minOccurs="3" maxOccurs="20"/>
