				</xsd:complexType>
			</xsd:element>
			<xsd:element name="r31viacAko4" type="only_0_1"/>
			<xsd:element name="r32" type="optDec"/>
			<xsd:element name="r32a" type="optDec"/>
			<xsd:element name="r33" type="optDec"/>
			<xsd:element name="r33a" type="optDec"/>
			<xsd:element name="r33b" type="optDec"/>
			<xsd:element name="r34" type="optDec"/>
			<xsd:element name="r35" type="optDec"/>
			<xsd:element name="r36" type="optDec"/>
